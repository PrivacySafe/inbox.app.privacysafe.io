    "@vue/eslint-config-typescript": "14.1.3",
    "@vuedx/typescript-plugin-vue": "0.7.6",
    "eslint": "9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-node": "11.1.0",
